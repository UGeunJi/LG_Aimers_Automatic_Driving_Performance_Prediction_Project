]     \n","    data2 = data[data[column] < lower]    \n","    \n","    # 이상치 총 개수 구하기\n","    return print('총 이상치 개수는', data1.shape[0] + data2.shape[0], '이다.')"],"metadata":{"id":"BuByBUTrZ7C2","executionInfo":{"status":"ok","timestamp":1659811223933,"user_tz":-540,"elapsed":374,"user":{"displayName":"우근","userId":"13959726526519675652"}}},"execution_count":479,"outputs":[]},{"cell_type":"code","source":["outlier_iqr(train_x,'X_01')\n","\n","data_X1 = train_x[(train_x['X_01'] < upper) & (train_x['X_01'] > lower)]\n","len(data_X1)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"epsZOSJ-gCQ8","executionInfo":{"status":"ok","timestamp":1659811227342,"user_tz":-540,"elapsed":359,"user":{"displayName":"우근","userId":"13959726526519675652"}},"outputId":"e75d8efa-fc17-4a29-cc41-a13902ffa209"},"execution_count":480,"outputs":[{"output_type":"stream","name":"stdout","text":["IQR은 3.0589999999999975 이다.\n","lower bound 값은 61.87650000000001 이다.\n","upper bound 값은 74.1125 이다.\n","총 이상치 개수는 1145 이다.\n"]},{"output_type":"execute_result","data":{"text/plain":["38462"]},"metadata":{},"execution_count":480}]},{"cell_type":"code","source":["plt.figure(figsize=(10,5))\n","\n","sns.distplot(train_x.X_01, kde=False)\n","\n","# 이상치 영역 박스 그리기\n","plt.axvspan(xmin=lower, xmax=train_x.X_01.min(), alpha=0.2, color='red')\n","plt.axvspan(xmin=upper, xmax=train_x.X_01.max(), alpha=0.2, color='red')"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":353},"id":"4VE9qIfNhTvk","executionInfo":{"status":"ok","timestamp":1659811231821,"user_tz":-540,"elapsed":402,"user":{"displayName":"우근","userId":"13959726526519675652"}},"outputId":"3d5e0f2a-d8d0-4439-ecd0-ae08f5ebeacc"},"execution_count":481,"outputs":[{"output_type":"execute_result","data":{"text/plain":["<matplotlib.patches.Polygon at 0x7f24a31fc5d0>"]},"metadata":{},"execution_count":481},{"output_type":"display_data","data":{"text/plain":["<Figure size 720x360 with 1 Axes>"],"image/png":"\n"},"metadata":{"n